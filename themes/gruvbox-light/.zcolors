COMMAND_PROMPT="#3c3836"

CHAR="%{$fg[green]%}"
ERROR="%{$fg[red]%}"
BGJOBS="%{$fg[blue]%}"
VCS_INFO="%{$fg[magenta]%}"
DIR="%{$fg[cyan]%}"

ZSH_HIGHLIGHT_STYLES_PRECOMMAND="green"
ZSH_HIGHLIGHT_STYLES_PATH_APPROX="yellow"

source ~/ui/vendor/vim/gruvbox/gruvbox_256palette.sh
